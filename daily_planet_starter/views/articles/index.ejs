<h1>Articles</h1>

<a href="/articles/new" class="btn btn-primary">New Article</a>

<% articles.forEach(function(article, id) { %>
  <div class="well">
    <a href="/articles/<%= id %>">
    <h2><%= article.title %></h2></a>
    <a href="/articles/<%=id%>/edit" class="btn btn-warning">
  		Edit
  	</a>
    <a  href="/articles/<%=id%>" id="delete-btn" class="btn btn-danger">
 		Delete 
  	</a>
  </div>
<% }); %>

